install-cglossaryDATA: $(cglossary_DATA)
	@$(NORMAL_INSTALL)
	test -z "$(cglossarydir)" || $(MKDIR_P) "$(DESTDIR)$(cglossarydir)"
	@list='$(cglossary_DATA)'; for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  f=$(am__strip_dir) \
	  echo " $(cglossaryDATA_INSTALL) '$$d$$p' '$(DESTDIR)$(cglossarydir)/$$f'"; \
	  $(cglossaryDATA_INSTALL) "$$d$$p" "$(DESTDIR)$(cglossarydir)/$$f"; \
	done

uninstall-cglossaryDATA:
	@$(NORMAL_UNINSTALL)
	@list='$(cglossary_DATA)'; for p in $$list; do \
	  f=$(am__strip_dir) \
	  echo " rm -f '$(DESTDIR)$(cglossarydir)/$$f'"; \
	  rm -f "$(DESTDIR)$(cglossarydir)/$$f"; \
	done
